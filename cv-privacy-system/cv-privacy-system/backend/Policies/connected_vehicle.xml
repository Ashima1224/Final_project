<ext:DATA-TYPE id="poi_interactions" optional="yes"/>
<ext:DATA-TYPE id="device_identifier" optional="yes"/>
</ext:DATA-TYPES>
</DATA-GROUP>
<ext:CONTEXT-REQUIREMENTS>
<ext:SITUATION id="daytime"/>
<ext:SITUATION id="night_time"/>
<ext:SITUATION id="residential"/>
