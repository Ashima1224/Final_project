<ext:DATA-TYPE id="location_precise_gps"/>
<ext:DATA-TYPE id="timestamp" optional="yes"/>
</ext:DATA-TYPES>
</DATA-GROUP>
<ext:CONTEXT-REQUIREMENTS>
<ext:SITUATION id="daytime"/>
<ext:SITUATION id="night_time"/>
</ext:CONTEXT-REQUIREMENTS>
<ext:PRIVACY-ACTIONS>
<ext:ACTION id="ALLOW"/>
<ext:ACTION id="GENERALIZE"/>
<ext:ACTION id="DELAY"/>
<ext:ACTION id="BLOCK"/>
</ext:PRIVACY-ACTIONS>
</STATEMENT>
<!-- ========================================================= -->
<!-- STATEMENT 6: ADVERTISEMENT (context: advertisement) -->
<!-- ========================================================= -->
