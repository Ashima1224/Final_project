<ext:SITUATION id="residential"/>
<ext:SITUATION id="high_speed"/>
</ext:CONTEXT-REQUIREMENTS>
<ext:PRIVACY-ACTIONS>
<ext:ACTION id="ALLOW"/>
<ext:ACTION id="GENERALIZE"/>
<ext:ACTION id="ANONYMIZE"/>
<ext:ACTION id="BLOCK"/>
<ext:ACTION id="K_ANONYMITY"/>
</ext:PRIVACY-ACTIONS>
</STATEMENT>
<!-- ========================================================= -->
<!-- STATEMENT 3: POI RECOMMENDATIONS (context: poi_recommendations) -->
<!-- ========================================================= -->
<STATEMENT>
<CONSEQUENCE>
Location and place interactions are used to recommend nearby points of interest.
</CONSEQUENCE>
<PURPOSE>
<tailoring required="opt-out"/>
<develop required="opt-out"/>
</PURPOSE>
<RECIPIENT>
<ours/>
<delivery/>
</RECIPIENT>
<RETENTION>
<stated-purpose/>
</RETENTION>
