<ext:SITUATION id="daytime"/>
<ext:SITUATION id="night_time"/>
</ext:CONTEXT-REQUIREMENTS>
<ext:PRIVACY-ACTIONS>
<ext:ACTION id="ALLOW"/>
<ext:ACTION id="ANONYMIZE"/>
<ext:ACTION id="LOCAL_ONLY"/>
<ext:ACTION id="BLOCK"/>
</ext:PRIVACY-ACTIONS>
</STATEMENT>
<!-- ========================================================= -->
<!-- STATEMENT 5: LOCATION SHARING (context: location_sharing) -->
<!-- ========================================================= -->
<STATEMENT>
<CONSEQUENCE>
Real-time location can be shared with contacts selected by the user.
</CONSEQUENCE>
<PURPOSE>
<contact required="opt-in"/>
</PURPOSE>
<RECIPIENT>
<unrelated/>
</RECIPIENT>
<RETENTION>
<no-retention/>
</RETENTION>
