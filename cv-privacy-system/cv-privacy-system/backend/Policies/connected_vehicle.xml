<!-- Standard P3P placeholder -->
<DATA ref="#dynamic.miscdata">
<CATEGORIES><physical/></CATEGORIES>
</DATA>
<!-- Domain vocabulary alignment -->
<ext:CONTEXT id="navigation"/>
<ext:DATA-TYPES>
<ext:DATA-TYPE id="location_precise_gps"/>
<ext:DATA-TYPE id="location_heading"/>
<ext:DATA-TYPE id="route_origin"/>
<ext:DATA-TYPE id="route_destination"/>
<ext:DATA-TYPE id="route_waypoints" optional="yes"/>
<ext:DATA-TYPE id="speed_current" optional="yes"/>
</ext:DATA-TYPES>
</DATA-GROUP>
<ext:CONTEXT-REQUIREMENTS>
<ext:SITUATION id="highway"/>
<ext:SITUATION id="residential"/>
