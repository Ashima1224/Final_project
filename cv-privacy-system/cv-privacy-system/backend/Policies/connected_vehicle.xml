<POLICY name="openstreetmap-osmf-policy-context-aware"
discuri="https://osmfoundation.org/wiki/Privacy_Policy">
<!-- ========== ENTITY INFO ========== -->
<ENTITY>
<DATA-GROUP>
<DATA ref="#business.name">OpenStreetMap (OSMF)</DATA>
<DATA ref="#business.contact-info.online.uri">https://osmfoundation.org/wiki/Privacy_Policy</DATA>
</DATA-GROUP>
</ENTITY>
<ACCESS>
<contact/>
</ACCESS>
<DISPUTES-GROUP>
<DISPUTES resolution-type="service"
service="https://osmfoundation.org/wiki/Privacy_Policy"
short-description="OSMF privacy policy and contact process."/>
</DISPUTES-GROUP>
<!-- ========================================================= -->
<!-- STATEMENT 1: MAP BROWSING / TILE DELIVERY (context: poi_recommendations) -->
<!-- ========================================================= -->
<STATEMENT>
<CONSEQUENCE>
Basic request and usage data is processed to deliver map tiles and website functionality.
</CONSEQUENCE>
<PURPOSE>
<current/>
<admin/>
</PURPOSE>
<RECIPIENT>
<ours/>
</RECIPIENT>
<RETENTION>
<business-practices/>
</RETENTION>
<ext:RETENTION-POLICY>
<ext:DURATION unit="days">7</ext:DURATION>
<ext:SCOPE>server-logs</ext:SCOPE>
<ext:DELETION>automatic</ext:DELETION>
</ext:RETENTION-POLICY>
<DATA-GROUP>
<DATA ref="#dynamic.http"/>
<DATA ref="#dynamic.clickstream"/>
<!-- Domain vocabulary alignment (contexts + data types) -->
<ext:CONTEXT id="poi_recommendations"/>
<ext:DATA-TYPES>
<ext:DATA-TYPE id="location_coarse" optional="yes"/>
<ext:DATA-TYPE id="timestamp" optional="yes"/>
<ext:DATA-TYPE id="device_identifier" optional="yes"/>
<ext:DATA-TYPE id="poi_interactions" optional="yes"/>
</ext:DATA-TYPES>
</DATA-GROUP>
<!-- Context requirements restricted to vocabulary situations only -->
<ext:CONTEXT-REQUIREMENTS>
<ext:SITUATION id="daytime"/>
<ext:SITUATION id="night_time"/>
<ext:SITUATION id="residential"/>
<ext:SITUATION id="highway"/>
</ext:CONTEXT-REQUIREMENTS>
</STATEMENT>
<!-- ========================================================= -->
<!-- STATEMENT 2: ANALYTICS / SERVICE IMPROVEMENT (context: personalization) -->
<!-- ========================================================= -->
<STATEMENT>
<CONSEQUENCE>
Aggregated usage signals may be used to understand site usage and improve performance and reliability.
</CONSEQUENCE>
<PURPOSE>
<develop required="opt-out"/>
<research required="opt-out"/>
</PURPOSE>
<RECIPIENT>
<ours/>
</RECIPIENT>
<RETENTION>
<stated-purpose/>
</RETENTION>
<ext:RETENTION-POLICY>
<ext:DURATION unit="days">180</ext:DURATION>
<ext:SCOPE>analytics</ext:SCOPE>
<ext:DELETION>automatic</ext:DELETION>
</ext:RETENTION-POLICY>
<DATA-GROUP>
<DATA ref="#dynamic.clickstream" optional="yes"/>
<DATA ref="#dynamic.http" optional="yes"/>
<DATA ref="#dynamic.cookies" optional="yes"/>
<!-- Domain vocabulary alignment -->
<ext:CONTEXT id="personalization"/>
<ext:DATA-TYPES>
<ext:DATA-TYPE id="poi_interactions" optional="yes"/>
<ext:DATA-TYPE id="search_history" optional="yes"/>
<ext:DATA-TYPE id="timestamp" optional="yes"/>
<ext:DATA-TYPE id="device_identifier" optional="yes"/>
<ext:DATA-TYPE id="location_coarse" optional="yes"/>
</ext:DATA-TYPES>
</DATA-GROUP>
<!-- Context requirements restricted to vocabulary situations only -->
<ext:CONTEXT-REQUIREMENTS>
<ext:SITUATION id="daytime"/>
<ext:SITUATION id="night_time"/>
<ext:SITUATION id="residential"/>
</ext:CONTEXT-REQUIREMENTS>
</STATEMENT>
<!-- ========================================================= -->
<!-- STATEMENT 3: EDITING / CONTRIBUTIONS (PUBLIC HISTORY) (context: personalization) -->
<!-- ========================================================= -->
<STATEMENT>
<CONSEQUENCE>
Account and contribution data is processed to manage edits and maintain attribution and a public edit history.
</CONSEQUENCE>
<PURPOSE>
<current/>
<admin/>
</PURPOSE>
<RECIPIENT>
<ours/>
<public/>
</RECIPIENT>
<RETENTION>
<indefinitely/>
</RETENTION>
<ext:RETENTION-POLICY>
<ext:DURATION unit="days">0</ext:DURATION>
<ext:SCOPE>public-history</ext:SCOPE>
<ext:DELETION>not-applicable</ext:DELETION>
</ext:RETENTION-POLICY>
<DATA-GROUP>
<DATA ref="#user.name"/>
<DATA ref="#dynamic.miscdata">
<CATEGORIES><other/></CATEGORIES>
</DATA>
<DATA ref="#dynamic.http" optional="yes"/>
<!-- Domain vocabulary alignment -->
<ext:CONTEXT id="personalization"/>
<ext:DATA-TYPES>
<ext:DATA-TYPE id="user_profile" optional="yes"/>
<ext:DATA-TYPE id="poi_interactions" optional="yes"/>
<ext:DATA-TYPE id="timestamp" optional="yes"/>
<ext:DATA-TYPE id="device_identifier" optional="yes"/>
</ext:DATA-TYPES>
</DATA-GROUP>
<!-- Context requirements restricted to vocabulary situations only -->
<ext:CONTEXT-REQUIREMENTS>
<ext:SITUATION id="daytime"/>
<ext:SITUATION id="night_time"/>
</ext:CONTEXT-REQUIREMENTS>
</STATEMENT>
</POLICY>
</POLICIES>
