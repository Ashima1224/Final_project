<ext:SITUATION id="night_time"/>
</ext:CONTEXT-REQUIREMENTS>
<ext:PRIVACY-ACTIONS>
<ext:ACTION id="ALLOW"/>
<ext:ACTION id="GENERALIZE"/>
<ext:ACTION id="ANONYMIZE"/>
<ext:ACTION id="BLOCK"/>
</ext:PRIVACY-ACTIONS>
</STATEMENT>
<!-- ========================================================= -->
<!-- STATEMENT 2: TRAFFIC ANALYTICS (context: traffic_analytics) -->
<!-- ========================================================= -->
<STATEMENT>
<CONSEQUENCE>
Aggregated location and speed data is used to improve real-time traffic information.
</CONSEQUENCE>
<PURPOSE>
<current/>
<develop required="opt-out"/>
</PURPOSE>
<RECIPIENT>
<ours/>
<delivery/>
</RECIPIENT>
<RETENTION>
<stated-purpose/>
</RETENTION>
<DATA-GROUP>
<DATA ref="#dynamic.miscdata" optional="yes">
<CATEGORIES><physical/></CATEGORIES>
</DATA>
