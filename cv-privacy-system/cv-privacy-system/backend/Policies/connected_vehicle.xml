<!-- ========================================================= -->
<!-- STATEMENT 1: NAVIGATION (context: navigation) -->
<!-- ========================================================= -->
<STATEMENT>
<CONSEQUENCE>
Precise location and route data is used to provide turn-by-turn navigation and route guidance.
</CONSEQUENCE>
<PURPOSE>
<current/>
</PURPOSE>
<RECIPIENT>
<ours/>
</RECIPIENT>
<RETENTION>
<stated-purpose/>
</RETENTION>
